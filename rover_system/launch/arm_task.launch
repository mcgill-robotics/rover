<launch>
    <!-- Start the node without specifying a name in the node tag -->
    <include file="$(find rosbridge_server)/launch/rosbridge_websocket.launch"/>

    <node pkg="camera_data" name="camera_pub_1" type="camera_publisher_1.py" output="screen">
    </node>

    <node pkg="camera_data" name="camera_pub_2" type="camera_publisher_2.py" output="screen">
    </node>

    <node pkg="odrive_interface" type="node_odrive_arm.py" name="odrive_interface_arm"
        output="screen">
    </node>

  </launch>